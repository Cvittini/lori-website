{
    "kind": "collectionType",
    "collectionName": "meals",
    "info": {
        "singularName": "meal",
        "pluralName": "meals",
        "displayName": "Meal"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "name": {
            "type": "string"
        },
        "slug": {
            "type": "uid"
        },
        "type": {
            "type": "enumeration",
            "enum": [
                "cookie,tea"
            ]
        },
        "flavor": {
            "type": "enumeration",
            "enum": [
                "tropical fruit, tropical, blues"
            ]
        },
        "image": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files",
                "videos",
                "audios"
            ]
        },
        "description": {
            "type": "richtext"
        },
        "isAvailabe": {
            "type": "boolean",
            "default": true
        },
        "packs": {
            "type": "component",
            "component": "packs.shop-pack-option",
            "repeatable": true
        },
        "seo": {
            "type": "component",
            "component": "seo.shared-seo",
            "repeatable": false
        }
    }
}
