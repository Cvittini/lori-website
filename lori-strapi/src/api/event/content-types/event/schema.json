{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date"
    },
    "time": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "includes": {
      "type": "string"
    },
    "price": {
      "type": "decimal"
    },
    "bannerImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::registration.registration",
      "mappedBy": "event"
    }
  }
}
